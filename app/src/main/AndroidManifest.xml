        android:icon="@mipmap/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/applogo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyRestaurentAdmin">
        <activity android:name=".showallproducts"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".AdminNewOrdersActivity" />
        <activity android:name=".AdminUserProductsActivity" />
        <activity android:name=".AdminAddNewProductActivity" />
    </application>

</manifest>